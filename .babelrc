{
  "presets": [
    ["@babel/preset-env", 
      { "modules": false, 
      "targets": "> 0.25%, not dead",
      "useBuiltIns": "entry",
      "corejs": "3.8" }],
    "@babel/preset-react"
  ],
  "plugins": [
    "@babel/plugin-syntax-dynamic-import",
    "@babel/plugin-proposal-class-properties",
    "react-hot-loader/babel"
  ]
}